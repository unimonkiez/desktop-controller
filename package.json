{
  "name": "desktop-controller",
  "version": "0.1.2",
  "description": "Desktop controller",
  "keywords": [
    "install",
    "npm"
  ],
  "homepage": "https://github.com/unimonkiez/desktop-controller",
  "bugs": {
    "url": "https://github.com/unimonkiez/desktop-controller/issues"
  },
  "license": "MIT",
  "author": "unimonkiez",
  "main": "./lib/index.js",
  "bin": "./lib/bin.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:unimonkiez/desktop-controller.git"
  },
  "scripts": {
    "build": "node ./bin/build.js",
    "build:prod": "npm run build -- -p",
    "build:watch": "npm run build -- -w",
    "build:prod:watch": "npm run build -- -p -w",
    "build:watch:prod": "npm run build:prod:watch",
    "lint": "eslint --ext .js,.jsx .",
    "start": "node ./bin/start.js -power 1 -reset 2 -wifi 3 -uv 4 -led 5,6,7",
    "test": "npm run lint",
    "predeploy": "rimraf dist && npm run build:prod  && node ./bin/npm-dependencies.js",
    "deploy": "npm publish",
    "postdeploy": "node ./bin/npm-dependencies.js --delete"
  },
  "dependencies": {
    "color": "^0.11.3",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fontgen-loader": "^0.2.1",
    "hoist-non-react-statics": "^1.2.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "material-ui": "^0.17.4",
    "react": "^15.5.0",
    "react-color": "^2.8.0",
    "react-dom": "^15.5.0",
    "react-tap-event-plugin": "^2.0.1",
    "replace-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "whatwg-fetch": "^1.0.0"
  },
  "npmDependencies": {
    "rpi-gpio": "^0.8.1"
  }
}
